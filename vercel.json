{
  "functions": {
    "api/v1/[...route].js": {
      "runtime": "nodejs18.x"
    }
  },
  "routes": [
    {
      "src": "/api/v1/(.*)",
      "dest": "/api/v1/[...route]?route=$1"
    },
    {
      "handle": "filesystem"
    },
    {
      "src": "/(.*)",
      "dest": "/index.html"
    }
  ]
}
